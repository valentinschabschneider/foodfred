{
	"name": "foodfred",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"supabase": "npx supabase",
		"supabase:types": "yarn supabase gen types typescript --schema public > types/Database.ts",
		"supabase:types:local": "yarn supabase:types --local",
		"supabase:types:linked": "npx supabase gen types typescript --schema public --linked > types/Database.ts"
	},
	"devDependencies": {
		"@prgm/sveltekit-progress-bar": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/kit": "^1.20.4",
		"autoprefixer": "^10.4.14",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.5.1",
		"@supabase/auth-helpers-sveltekit": "^0.10.2",
		"@supabase/supabase-js": "^2.31.0",
		"@sveltestack/svelte-query": "^1.6.0",
		"dayjs": "^1.11.9",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.40.2",
		"flowbite-svelte-icons": "^0.3.5",
		"supabase": "^1.83.7",
		"svelte-copy": "^1.4.1",
		"svelte-sonner": "^0.1.3",
		"tailwind-merge": "^1.14.0"
	}
}
